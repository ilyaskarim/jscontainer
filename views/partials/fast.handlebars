<script type="text/javascript">
    let links = [
        "https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800&display=swap",
        "https://cdn.jsdelivr.net/npm/flexboxgrid@6.3.1/dist/flexboxgrid.min.css",
        "https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css",
        "https://cdn.jsdelivr.net/npm/@mdi/font@5.8.55/css/materialdesignicons.min.css",
        "https://fonts.googleapis.com/css2?family=Inter:wght@100;200;300;500;600;700;800;900&amp;display=swap",
        "https://fonts.googleapis.com/css2?family=Monoton&display=swap",
        "https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700&display=swap",
        "https://fonts.googleapis.com/css2?family=Roboto+Slab:wght@500;600&display=swap",
        "https://fonts.googleapis.com/css2?family=Lato:wght@100;300;400;700&display=swap",
        "https://fonts.gstatic.com",
        'https://cdnjs.cloudflare.com/ajax/libs/monaco-editor/0.25.0/min/vs/editor/editor.main.min.css'
    ];
    let jsLinks = [
        "https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.29.1/moment.min.js",
        "https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js",
        "/pb/js/index.js",
    ];
    setTimeout(() => {
        links.forEach((item) => {
            let tag = document.createElement("link");
            tag.setAttribute("rel", "stylesheet");
            tag.setAttribute("href", item);
            document.querySelector(".js-css").append(tag);
        });
        jsLinks.forEach((item) => {
            let tag = document.createElement("script");
            tag.setAttribute("src", item);
            if (item.includes("bootstrap.bundle.min.js")) {
                tag.onload = () => {
                    var popoverTriggerList = [].slice.call(document.querySelectorAll('[data-bs-toggle="popover"]'))
                    console.log(popoverTriggerList)
                    var popoverList = popoverTriggerList.map(function (popoverTriggerEl) {
                        return new bootstrap.Popover(popoverTriggerEl)
                    })
                }
            }
            document.body.append(tag);
        });
    },300)
</script>

<script>
    document.addEventListener("DOMContentLoaded", function () {
        var lazyloadImages = document.querySelectorAll(".lazy-load-photo");
        setTimeout(() => {
            lazyloadImages.forEach((img) => {
                img.src = img.getAttribute("data-src");
            })
        }, 1)
    });
</script>